plugins:
  - '@babel/plugin-proposal-optional-chaining'
  - '@babel/plugin-proposal-nullish-coalescing-operator'
  - - '@babel/plugin-proposal-class-properties'
    - loose: true
presets:
  - '@babel/preset-flow'
  - '@babel/preset-react'
  - - '@babel/preset-env'
    - modules: false
      shippedProposals: true
      loose: true
      useBuiltIns: false
